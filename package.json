{
  "name": "workflow-reports-action",
  "version": "1.0.1",
  "private": true,
  "description": "Generate reports on how a user or organization uses GitHub Actions",
  "main": "dist/main.js",
  "scripts": {
    "build": "ncc build src/main.ts --source-map --license licenses.txt",
    "lint": "ts-standard src/*.ts",
    "lintfix": "npm run lint -- --fix",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjohnbo/workflow-reports-action.git"
  },
  "keywords": [
    "actions",
    "github",
    "workflow",
    "reports"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "^0.5.0",
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.10",
    "@typescript-eslint/parser": "^4.15.0",
    "@vercel/ncc": "^0.25.1",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "ts-jest": "^26.5.4",
    "ts-standard": "^10.0.0",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && git add dist/"
    }
  }
}
